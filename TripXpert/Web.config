  <system.web>
    <authentication mode="None" />
    <httpRuntime targetFramework="4.5" />
    <compilation debug="true" targetFramework="4.8" />
    <pages>
      <namespaces>
        <add namespace="Kendo.Mvc.UI" />
      </namespaces>
    </pages>
  </system.web>
  <system.webServer>
    <modules>
      <remove name="FormsAuthentication" />
